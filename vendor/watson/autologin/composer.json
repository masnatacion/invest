    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.*",
        "nesbot/carbon": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "classmap": [
            "src/controllers"
